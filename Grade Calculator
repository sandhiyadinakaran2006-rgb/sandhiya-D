importjava.util.*;
class gradeCalculator{
public static void main(string[]args){
Scanner sc=new Scanner(system.in);
int sum=0;
for(int i=0;i<5;i++)sum +=sc.nextlnt();
int avg=sum/5;
System.out.println("Average:"+avg);
System.out.println(avg>=90?"GradeA":
avg>=75?"GradeB":
avg>=60?"GradeC":"GradeD");
}
}
